# Environment Variables Configuration
# For production deployment guide, see DEPLOYMENT.md

MAILGUN_API_KEY=your-mailgun-api-key
MAILGUN_SENDER_DOMAIN=your-mailgun-sender-domain

DATABASE_URL=postgres://myuser:mypassword@localhost:5432/mydatabase

SECRET_KEY=dummy-ci-secret-key

# Set to True for local development, False for production
DEBUG=True

DJANGO_SETTINGS_MODULE=nbxdjango.settings

# Frontend URL for password reset links
FRONTEND_URL=http://localhost:3000

# CORS - Comma-separated list of allowed origins
CORS_ALLOWED_ORIGINS=http://localhost:3000,https://app.example.com

# Narbox Logo URL for email templates
# Must be a publicly accessible URL (e.g., https://yourdomain.com/static/packagehandling/images/narbox-logo.png)
NARBOX_LOGO_URL=
